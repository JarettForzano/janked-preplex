{
  "name": "search-engine",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "concurrently \"vite\" \"npm run start-js-server\" \"npm run start-python-server\"",
    "start-react": "vite",
    "start-js-server": "nodemon --watch js-server/ --exec 'node -r dotenv/config js-server/server.js'",
    "start-python-server": "nodemon --exec python3 python-server/server.py"
  },
